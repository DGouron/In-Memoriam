{
  "name": "in-memoriam",
  "version": "0.1.2",
  "description": "Cartographie des décès militaires et civils Françaises",
  "main": "index.html",
  "repository": "https://github.com/Geolim4/In-Memoriam.git",
  "author": "Georges.L <contact@geolim4.com>",
  "license": "GPL-2.0",
  "private": false,
  "scripts": {
    "build": "yarn run lint && npx webpack",
    "lint": "tslint --project ./tsconfig.json",
    "lint-fix": "tslint --project ./tsconfig.json --fix",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "extensions": "ts",
      "ignore": [
        "dist",
        "node_modules"
      ],
      "patterns": [
        "src/ts"
      ]
    }
  },
  "dependencies": {
    "@google/markerclusterer": "^1.0.3",
    "load-google-maps-api": "^2.0.1",
    "qwest": "^4.5.0",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@types/googlemaps": "^3.37.4",
    "@types/markerclustererplus": "^2.1.33",
    "@types/node": "^12.7.2",
    "@types/qwest": "^1.7.28",
    "npm-watch": "^0.6.0",
    "path": "^0.12.7",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7"
  }
}
